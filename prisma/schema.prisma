generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model User {
  id    String @id @default(cuid())
  name  String
  email String @unique

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}
